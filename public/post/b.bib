	Issn = {00223433},
	Journal = {Journal of Peace Research},
	Number = {4},
	Pages = {377--390},
	Title = {The {Doctrine} of {Economic} {Neutrality} of the {IMF} and the {World} {Bank}},
	Url = {http://www.jstor.org/stable/424107},
