	Journal = {IMF Staff Papers},
	Number = {2},
	Pages = {260--286},
	Title = {{IMF} {Programs} and {Growth}: {Is} {Optimism} {Defensible}?},
	Url = {http://www.jstor.org/stable/30035898},
