	Number = {2},
	Pages = {473--494},
	Title = {Government {Spending} and {Public} {Support} for {Trade} in the {OECD}: {An} {Empirical} {Test} of the {Embedded} {Liberalism} {Thesis}},
	Url = {http://www.jstor.org/stable/3877913},
