@article{hibbing1987accounting,
	Abstract = {British political parties are usually capable of dictating the voting behavior of their members in the House of Commons, but there are instances in which the parties choose to grant MPs freedom to vote as they wish. These "free votes" occur on private members' bills and frequently involve extremely emotional, important issues. Using multiple regression analysis, we examine voting on selected free votes since 1966. The results suggest that while partisan forces are potent even on free votes, they are by no means the only forces operating. Personal characteristics of the MPs\&\#x2014;such as religion, age, education, and the like\&\#x2014;also influence voting decisions. Moreover, at times it is even possible to observe characteristics of the constituencies (religion, region, and marginality) affecting voting decisions. We conclude by discussing what these findings mean for views of decision making in the House of Commons.},
	Author = {Hibbing, John R. and Marsh, David},
