@article{wendt1992anarchy,
	Abstract = {It is informed that the debate between realists and liberals has reemerged as an axis of contention in international relations theory. This article deliberates on to what extent the state action is influenced by "structure" versus "process" and institutions. The debate between "neorealists" and "neoliberals" has been based on a shared commitment to "rationalism." Classical realists such as Thomas Hobbes, Reinhold Niebuhr, and Hans Morgenthau attributed egoism and power politics primarily to human nature, whereas structural realists or neorealists emphasize anarchy. The difference stems in part from different interpretations of anarchy's causal powers. It is opined that self-help is an institution, one of various structures of identity and interest that may exist under anarchy. Processes of identity-formation under anarchy are concerned first and foremost with preservation or "security" of the self. Concepts of security therefore differ in the extent to which and the manner in which the self is identified cognitively with the other, and, the author wants to suggest, it is upon this cognitive variation that the meaning of anarchy and the distribution of power depends.},
	Author = {Wendt, Alexander},
	Date-Added = {2018-01-22 19:05:58 +0000},
	Date-Modified = {2018-01-22 19:19:58 +0000},
	Journal = {International Organization},
	Keywords = {BALANCE of power, EGOISM, HOBBES, Thomas, 1588-1679, INTERNATIONAL relations, MORGENTHAU, Hans, NIEBUHR, Reinhold, 1892-1971},
	Number = {2},
	Pages = {391},
	Title = {Anarchy is what states make of it: {The} social construction of power politics.},
	Volume = {46},
	Year = {1992}}

