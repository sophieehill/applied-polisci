@article{hawkins2003populism,
	Abstract = {In the 1990s Venezuela experienced the rise of a new anti-party movement built around the figure of Hugo Ch\&\#xe1;vez and dedicated to the fundamental transformation of society, a movement that most Venezuelans call Chavismo. If we define populism in strictly political terms--as the presence of what some scholars call a charismatic mode of linkage between voters and politicians, and a democratic discourse that relies on the idea of a popular will and a struggle between 'the people' and 'the elite'--then Chavismo is clearly a populist phenomenon. Chavismo relies on charismatic linkages between voters and politicians, a relationship largely unmediated by any institutionalised party. It also bases itself on a powerful, Manichaean discourse of 'the people versus the elite' that naturally encourages an 'anything goes' attitude among Ch\&\#xe1;vez's supporters. In this paper I demonstrate these points through a descriptive account, based on interviews performed in Caracas during autumn 1999, May 2000 and February 2003, as well as on published texts available in Venezuela. I also use this account to support an analytical claim that these populist qualities undermine Chavismo's democratic goals.},
	Author = {Hawkins, Kirk},
	Date-Added = {2018-01-22 19:05:58 +0000},
	Date-Modified = {2018-01-22 19:19:58 +0000},
	Issn = {01436597, 13602241},
	Journal = {Third World Quarterly},
	Number = {6},
	Pages = {1137--1160},
	Title = {Populism in {Venezuela}: {The} {Rise} of {Chavismo}},
	Volume = {24},
	Year = {2003}}

